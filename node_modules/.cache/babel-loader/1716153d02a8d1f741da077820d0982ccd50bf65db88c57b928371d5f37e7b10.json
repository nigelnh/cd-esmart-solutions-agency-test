     </span>\n            <button\n              class=\"delete-button\"\n              @click.stop=\"confirmDeleteProject(project.id)\"\n              title=\"Delete project\"\n            >\n              <i class=\"fa-solid fa-trash\"></i>\n            </button>\n          </div>\n        </div>\n        <p class=\"description\">{{ project.description || \"No description\" }}</p>\n        <div class=\"content-meta\">\n          <span>{{ formatDate(project.createdAt) }}</span>\n          <span>{{ project.type }}</span>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"goToHome\">Back to Home</button>\n      <button class=\"primary-button\" @click=\"$emit('next')\">\n        Create New Content\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"DashboardStep\",\n  data() {\n    return {\n      projects: [],\n      loading: true,\n      activeTab: \"projects\",\n      deleteConfirmation: {\n        show: false,\n        projectId: null,\n      },\n    };\n  },\n  created() {\n    this.fetchProjects();\n  },\n  methods: {\n    async fetchProjects() {\n      try {\n        this.loading = true;\n\n        const response = await axios.get(\"/api/projects\");\n\n        if (response.data.success) {\n          this.projects = response.data.data;\n        } else {\n          console.error(\"Failed to load projects\");\n          this.projects = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching projects:\", error);\n        this.projects = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString(undefined, {\n        year: \"numeric\",\n        month: \"short\",\n        day: \"numeric\",\n      });\n    },\n    viewProject(projectId) {\n      // Emit event to parent component to load the project for editing\n      this.$emit(\"edit-project\", projectId);\n    },\n    goToHome() {\n      this.$router.push(\"/\");\n    },\n    confirmDeleteProject(projectId) {\n      if (confirm(\"Are you sure you want to delete this project?\")) {\n        this.deleteProject(projectId);\n      }\n    },\n    async deleteProject(projectId) {\n      try {\n        const response = await axios.delete(`/api/projects/${projectId}`);\n\n        if (response.data.success) {\n          // Remove the project from the projects array\n          this.projects = this.projects.filter(\n            (project) => project.id !== projectId\n          );\n\n          // Show success message (optional)\n          alert(\"Project deleted successfully\");\n        } else {\n          console.error(\"Failed to delete project\");\n          alert(\"Failed to delete project\");\n        }\n      } catch (error) {\n        console.error(\"Error deleting project:\", error);\n        alert(\"An error occurred while deleting the project\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.step-content {\n  background: white;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n  color: #1c1c4c;\n  margin-bottom: 2rem;\n  text-align: center;\n}\n\n.content-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\n  gap: 1.5rem;\n  margin-bottom: 2rem;\n}\n\n.content-card {\n  background: #f8f9fa;\n  border-radius: 8px;\n  padding: 1.5rem;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  cursor: pointer;\n  /* border-left: 4px solid #1c1c4c; */\n}\n\n.content-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n}\n\n.content-card.finished {\n  border-left-color: #28a745;\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  margin-bottom: 0.5rem;\n}\n\n.card-actions {\n  display: flex;\n  align-items: center;\n  gap: 0.5rem;\n}\n\n.delete-button {\n  background: none;\n  border: none;\n  color: #dc3545;\n  cursor: pointer;\n  font-size: 0.9rem;\n  padding: 0.25rem 0.5rem;\n  border-radius: 4px;\n  transition: background-color 0.3s ease;\n}\n\n.delete-button:hover {\n  background-color: rgba(220, 53, 69, 0.1);\n}\n\n.card-header h3 {\n  margin: 0;\n  font-size: 1.1rem;\n  max-width: 70%;\n}\n\n.status-badge,\n.content-type-badge {\n  font-size: 0.75rem;\n  padding: 0.25rem 0.5rem;\n  border-radius: 12px;\n  font-weight: bold;\n}\n\n.status-badge.in_progress {\n  background-color: #ecedf7;\n  color: #1c1c4c;\n}\n\n.status-badge.finished {\n  background-color: #e8f5e9;\n  color: #28a745;\n}\n\n.content-type-badge {\n  background-color: #e3f2fd;\n  color: #0d47a1;\n}\n\n.description {\n  color: #6c757d;\n  margin-bottom: 1rem;\n  font-size: 0.9rem;\n  height: 40px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  /* -webkit-box-orient: vertical; */\n}\n\n.keywords-list {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 0.5rem;\n  margin-bottom: 1rem;\n}\n\n.keyword-tag {\n  background-color: #f0f0f0;\n  color: #666;\n  font-size: 0.75rem;\n  padding: 0.2rem 0.5rem;\n  border-radius: 12px;\n}\n\n.content-meta {\n  display: flex;\n  justify-content: space-between;\n  font-size: 0.8rem;\n  color: #6c757d;\n  border-top: 1px solid #eee;\n  padding-top: 0.75rem;\n}\n\n.empty-state,\n.loading-indicator {\n  grid-column: 1 / -1;\n  text-align: center;\n  padding: 2rem;\n  color: #6c757d;\n}\n\n.primary-button {\n  background: #1c1c4c;\n  color: white;\n  border: none;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: background-color 0.3s ease;\n}\n\n.primary-button:hover {\n  background: #2a2a6c;\n}\n\n.secondary-button {\n  background: transparent;\n  color: #1c1c4c;\n  border: 1px solid #1c1c4c;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: all 0.3s ease;\n}\n\n.secondary-button:hover {\n  background: #f8f9fa;\n}\n\n.button-group {\n  display: flex;\n  gap: 1rem;\n  justify-content: center;\n}\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAc;gEACvBC,mBAAA,CAAkB,YAAd,WAAS;;EAFjBC,GAAA;EAKyCF,KAAK,EAAC;;;EAL/CE,GAAA;EAM0BF,KAAK,EAAC;;gEACxBC,mBAAA,CAA0B,WAAvB,qBAAmB;oBAAtBE,UAA0B,C;;EAPlCD,GAAA;EAS6CF,KAAK,EAAC;;gEAC3CC,mBAAA,CAAoE,WAAjE,iEAA6D;oBAAhEG,UAAoE,C;oBAV5E;;EAoBaJ,KAAK,EAAC;AAAa;;EAEjBA,KAAK,EAAC;AAAc;oBAtBnC;iEAiCcC,mBAAA,CAAiC;EAA9BD,KAAK,EAAC;AAAmB;qBAA5BK,WAAiC,C;;EAIpCL,KAAK,EAAC;AAAa;;EACjBA,KAAK,EAAC;AAAc;;EAOxBA,KAAK,EAAC;AAAc;;uBA5C3BM,mBAAA,CAkDM,OAlDNC,UAkDM,GAjDJC,UAAkB,EAElBC,mBAAA,kBAAqB,EACVC,KAAA,CAAAC,SAAS,mB,cAApBL,mBAAA,CAsCM,OAtCNM,UAsCM,GArCOF,KAAA,CAAAG,OAAO,I,cAAlBP,mBAAA,CAEM,OAFNQ,UAEM,EARZ,IAAAC,UAAA,MASsBL,KAAA,CAAAM,QAAQ,CAACC,MAAM,U,cAA/BX,mBAAA,CAEM,OAFNY,UAEM,EAXZ,IAAAC,UAAA,O,kBAYMb,mBAAA,CA8BMc,SAAA;IA1CZlB,GAAA;EAAA,GAAAmB,WAAA,CAc0BX,KAAA,CAAAM,QAAQ,EAAnBM,OAAO;yBAFhBhB,mBAAA,CA8BM;MA3BHJ,GAAG,EAAEoB,OAAO,CAACC,EAAE;MAChBvB,KAAK,EAhBbwB,eAAA,EAgBc,cAAc;QAAAC,QAAA,EACAH,OAAO,CAACI,MAAM;MAAA;MACjCC,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,WAAW,CAACR,OAAO,CAACC,EAAE;QAE9BtB,mBAAA,CAgBM,OAhBN8B,WAgBM,GAfJ9B,mBAAA,CAA4B,YAAA+B,gBAAA,CAArBV,OAAO,CAACW,KAAK,kBACpBhC,mBAAA,CAaM,OAbNiC,WAaM,GAZJjC,mBAAA,CAIO;MAJDD,KAAK,EAvBvBwB,eAAA,EAuBwB,cAAc,EAASF,OAAO,CAACI,MAAM;wBAE7CJ,OAAO,CAACI,MAAM,wEAGlBzB,mBAAA,CAMS;MALPD,KAAK,EAAC,eAAe;MACpB2B,OAAK,EA9BpBQ,cAAA,CAAAP,MAAA,IA8B2BC,QAAA,CAAAO,oBAAoB,CAACd,OAAO,CAACC,EAAE;MAC5CU,KAAK,EAAC;OA/BpB,IAAAI,WAAA,kBAAAC,WAAA,E,KAqCQrC,mBAAA,CAAwE,KAAxEsC,WAAwE,EAAAP,gBAAA,CAA9CV,OAAO,CAACkB,WAAW,sCAC7CvC,mBAAA,CAGM,OAHNwC,WAGM,GAFJxC,mBAAA,CAAgD,cAAA+B,gBAAA,CAAvCH,QAAA,CAAAa,UAAU,CAACpB,OAAO,CAACqB,SAAS,mBACrC1C,mBAAA,CAA+B,cAAA+B,gBAAA,CAAtBV,OAAO,CAACsB,IAAI,iB,2BAxC/BC,WAAA;sCAAApC,mBAAA,gBA6CIR,mBAAA,CAKM,OALN6C,WAKM,GAJJ7C,mBAAA,CAAwE;IAAhED,KAAK,EAAC,kBAAkB;IAAE2B,OAAK,EAAAoB,MAAA,QAAAA,MAAA,U