{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"DashboardStep\",\n  data() {\n    return {\n      projects: [],\n      contents: [],\n      loading: true,\n      activeTab: \"projects\",\n      deleteConfirmation: {\n        show: false,\n        projectId: null\n      },\n      toast: {\n        show: false,\n        message: \"\",\n        type: \"info\",\n        // info, success, error\n        timeout: null\n      },\n      // Thay thế projectMenu bằng các biến riêng biệt\n      showProjectMenu: false,\n      projectMenuId: null,\n      projectMenuTitle: \"\",\n      // New variables for content menu\n      showContentMenu: false,\n      contentMenuId: null,\n      contentMenuTitle: \"\"\n    };\n  },\n  created() {\n    this.fetchProjects();\n  },\n  methods: {\n    async fetchProjects() {\n      try {\n        this.loading = true;\n        const response = await axios.get(\"/api/projects\");\n        if (response.data.success) {\n          this.projects = response.data.data;\n        } else {\n          console.error(\"Failed to load projects\");\n          this.projects = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching projects:\", error);\n        this.projects = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    async fetchContents() {\n      try {\n        this.loading = true;\n        const response = await axios.get(\"/api/content/contents\");\n        if (response.data.success) {\n          this.contents = response.data.data;\n        } else {\n          console.error(\"Failed to load contents\");\n          this.contents = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching contents:\", error);\n        this.contents = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString(undefined, {\n        year: \"numeric\",\n        month: \"short\",\n        day: \"numeric\"\n      });\n    },\n    truncateContent(content) {\n      if (!content) return \"\";\n\n      // Remove markdown/HTML formatting\n      const plainText = content.replace(/#+\\s|<[^>]+>/g, \"\");\n\n      // Limit to ~100 characters\n      if (plainText.length <= 100) return plainText;\n      return plainText.substring(0, 97) + \"...\";\n    },\n    // Process image URL to ensure it's absolute\n    ensureAbsoluteUrl(url) {\n      if (!url) return null;\n\n      // Return as-is if already absolute\n      if (url.startsWith(\"http://\") || url.startsWith(\"https://\")) {\n        return url;\n      }\n\n      // For relative URLs starting with '/api'\n      if (url.startsWith(\"/api\")) {\n        return `${window.location.origin}${url}`;\n      }\n\n      // Default fallback - prefix with origin\n      return `${window.location.origin}${url}`;\n    },\n    viewProject(projectId) {\n      // Tìm project để hiển thị tên\n      const project = this.projects.find(p => p.id === projectId);\n\n      // Hiển thị menu lựa chọn bằng các biến riêng biệt\n      this.showProjectMenu = true;\n      this.projectMenuId = projectId;\n      this.projectMenuTitle = project ? project.title : \"Project\";\n      console.log(\"Showing project menu for:\", projectId);\n    },\n    viewContent(contentId) {\n      // Find content to display name\n      const content = this.contents.find(c => c.id === contentId);\n\n      // Show content menu\n      this.showContentMenu = true;\n      this.contentMenuId = contentId;\n      this.contentMenuTitle = content ? content.topic || \"Content\" : \"Content\";\n      console.log(\"Showing content menu for:\", contentId);\n    },\n    closeProjectMenu() {\n      this.showProjectMenu = false;\n    },\n    closeContentMenu() {\n      this.showContentMenu = false;\n    },\n    goToEditor(projectId) {\n      console.log(\"Going to editor for project:\", projectId);\n      this.$emit(\"edit-project\", projectId);\n      this.closeProjectMenu();\n    },\n    goToFinal(projectId) {\n      console.log(\"Going to final view for project:\", projectId);\n      this.$emit(\"view-final\", projectId);\n      this.closeProjectMenu();\n    },\n    goToContentFinal(contentId) {\n      console.log(\"Going to final view for content:\", contentId);\n      this.$emit(\"view-final\", contentId, \"content\");\n      this.closeContentMenu();\n    },\n    goToHome() {\n      this.$router.push(\"/\");\n    },\n    createNewContent() {\n      // Emit event to notify that we want to create new content with reset data\n      this.$emit(\"create-new\");\n    },\n    // Debug method to check project ID format\n    debugProjectId(projectId) {\n      console.log(\"Project ID value:\", projectId);\n      console.log(\"Project ID type:\", typeof projectId);\n      console.log(\"Project ID length:\", projectId ? projectId.length : \"N/A\");\n\n      // Check if ID is in UUID format (example check)\n      const uuidPattern = /^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i;\n      console.log(\"Is UUID format:\", uuidPattern.test(projectId));\n      return projectId;\n    },\n    // Show toast message\n    showToast(message, type = \"info\") {\n      // Clear any existing timeout\n      if (this.toast.timeout) {\n        clearTimeout(this.toast.timeout);\n      }\n\n      // Set toast properties\n      this.toast.show = true;\n      this.toast.message = message;\n      this.toast.type = type;\n\n      // Auto hide after 3 seconds\n      this.toast.timeout = setTimeout(() => {\n        this.toast.show = false;\n      }, 3000);\n    },\n    confirmDeleteProject(event, projectId) {\n      // Ngăn chặn sự kiện click lan truyền đến card\n      event.stopPropagation();\n\n      // Debug project ID\n      this.debugProjectId(projectId);\n      if (confirm(this.$t(\"creatorAI.dashboard.deleteConfirm\"))) {\n        this.deleteProject(projectId);\n      }\n    },\n    confirmDeleteContent(event, contentId) {\n      // Prevent event propagation\n      event.stopPropagation();\n      if (confirm(\"Are you sure you want to delete this content? This cannot be undone.\")) {\n        this.deleteContent(contentId);\n      }\n    },\n    async deleteProject(projectId) {\n      try {\n        console.log(\"Deleting project with ID:\", projectId);\n\n        // Đảm bảo projectId không null và là string\n        if (!projectId) {\n          console.error(\"Project ID is null or undefined\");\n          this.showToast(this.$t(\"creatorAI.dashboard.deleteError\"), \"error\");\n          return;\n        }\n\n        // Log URL để debug\n        const url = `/api/projects/${projectId}`;\n        console.log(\"Delete URL:\", url);\n        const response = await axios({\n          method: \"DELETE\",\n          url: url,\n          headers: {\n            \"Content-Type\": \"application/json\"\n          }\n        });\n        console.log(\"Delete response:\", response);\n        if (response.data && response.data.success) {\n          // Remove the project from the projects array\n          this.projects = this.projects.filter(project => project.id !== projectId);\n\n          // Show success message\n          this.showToast(this.$t(\"creatorAI.dashboard.deleteSuccess\"), \"success\");\n\n          // Refresh projects list\n          this.fetchProjects();\n        } else {\n          console.error(\"Failed to delete project\", response.data);\n          this.showToast(this.$t(\"creatorAI.dashboard.deleteFailed\"), \"error\");\n        }\n      } catch (error) {\n        console.error(\"Error deleting project:\", error);\n\n        // Log chi tiết lỗi để debug\n        if (error.response) {\n          console.error(\"Error response:\", error.response.data);\n          console.error(\"Error status:\", error.response.status);\n        }\n        this.showToast(`${this.$t(\"creatorAI.dashboard.deleteError\")}: ${error.message}`, \"error\");\n      }\n    },\n    async deleteContent(contentId) {\n      try {\n        console.log(\"Deleting content with ID:\", contentId);\n        if (!contentId) {\n          console.error(\"Content ID is null or undefined\");\n          this.showToast(\"Delete error: Invalid content ID\", \"error\");\n          return;\n        }\n        const url = `/api/content/contents/${contentId}`;\n        console.log(\"Delete URL:\", url);\n        const response = await axios({\n          method: \"DELETE\",\n          url: url,\n          headers: {\n            \"Content-Type\": \"application/json\"\n          }\n        });\n        console.log(\"Delete response:\", response);\n        if (response.data && response.data.success) {\n          // Remove the content from the contents array\n          this.contents = this.contents.filter(content => content.id !== contentId);\n\n          // Show success message\n          this.showToast(\"Content deleted successfully\", \"success\");\n\n          // Refresh contents list\n          this.fetchContents();\n        } else {\n          console.error(\"Failed to delete content\", response.data);\n          this.showToast(\"Failed to delete content\", \"error\");\n        }\n      } catch (error) {\n        console.error(\"Error deleting content:\", error);\n\n        // Log detailed error for debugging\n        if (error.response) {\n          console.error(\"Error response:\", error.response.data);\n          console.error(\"Error status:\", error.response.status);\n        }\n        this.showToast(`Delete error: ${error.message}`, \"error\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","projects","contents","loading","activeTab","deleteConfirmation","show","projectId","toast","message","type","timeout","showProjectMenu","projectMenuId","projectMenuTitle","showContentMenu","contentMenuId","contentMenuTitle","created","fetchProjects","methods","response","get","success","console","error","fetchContents","formatDate","dateString","date","Date","toLocaleDateString","undefined","year","month","day","truncateContent","content","plainText","replace","length","substring","ensureAbsoluteUrl","url","startsWith","window","location","origin","viewProject","project","find","p","id","title","log","viewContent","contentId","c","topic","closeProjectMenu","closeContentMenu","goToEditor","$emit","goToFinal","goToContentFinal","goToHome","$router","push","createNewContent","debugProjectId","uuidPattern","test","showToast","clearTimeout","setTimeout","confirmDeleteProject","event","stopPropagation","confirm","$t","deleteProject","confirmDeleteContent","deleteContent","method","headers","filter","status"],"sources":["/Users/nhannguyen/Desktop/Duan/esmart-solutions-agency/src/components/creator-ai/DashboardStep.vue"],"sourcesContent":["<template>\n  <div class=\"step-content\">\n    <h2>{{ $t(\"creatorAI.dashboard.title\") }}</h2>\n\n    <!-- Toast Message -->\n    <div v-if=\"toast.show\" class=\"toast-message\" :class=\"toast.type\">\n      <span>{{ toast.message }}</span>\n      <button @click=\"toast.show = false\" class=\"close-toast\">&times;</button>\n    </div>\n\n    <!-- Tab Navigation -->\n    <div class=\"tab-navigation\">\n      <button\n        class=\"tab-button\"\n        :class=\"{ active: activeTab === 'projects' }\"\n        @click=\"activeTab = 'projects'\"\n      >\n        {{ $t(\"creatorAI.dashboard.projectsTab\") || \"Projects\" }}\n      </button>\n      <button\n        class=\"tab-button\"\n        :class=\"{ active: activeTab === 'content' }\"\n        @click=\"\n          activeTab = 'content';\n          fetchContents();\n        \"\n      >\n        {{ $t(\"creatorAI.dashboard.contentsTab\") || \"Content\" }}\n      </button>\n    </div>\n\n    <!-- Projects Tab -->\n    <div v-if=\"activeTab === 'projects'\" class=\"content-grid\">\n      <div v-if=\"loading\" class=\"loading-indicator\">\n        <p>{{ $t(\"creatorAI.dashboard.loading\") }}</p>\n      </div>\n      <div v-else-if=\"projects.length === 0\" class=\"empty-state\">\n        <p>{{ $t(\"creatorAI.dashboard.noProjects\") }}</p>\n      </div>\n      <div\n        v-else\n        v-for=\"project in projects\"\n        :key=\"project.id\"\n        class=\"content-card\"\n        :class=\"{ finished: project.status === 'finished' }\"\n        @click=\"viewProject(project.id)\"\n      >\n        <div class=\"card-header\">\n          <h3>{{ project.title }}</h3>\n          <div class=\"card-actions\">\n            <span class=\"status-badge\" :class=\"project.status\">\n              {{\n                project.status === \"in_progress\"\n                  ? $t(\"creatorAI.dashboard.inProgress\")\n                  : $t(\"creatorAI.dashboard.finished\")\n              }}\n            </span>\n            <button\n              class=\"delete-button\"\n              @click.stop=\"confirmDeleteProject($event, project.id)\"\n              title=\"Delete project\"\n            >\n              <i class=\"fa-solid fa-trash\"></i>\n            </button>\n          </div>\n        </div>\n        <p class=\"description\">\n          {{ project.description || $t(\"creatorAI.dashboard.noDescription\") }}\n        </p>\n        <div class=\"content-meta\">\n          <span>{{ formatDate(project.createdAt) }}</span>\n          <span>{{ project.type }}</span>\n        </div>\n      </div>\n    </div>\n\n    <!-- Contents Tab -->\n    <div v-if=\"activeTab === 'content'\" class=\"content-grid\">\n      <div v-if=\"loading\" class=\"loading-indicator\">\n        <p>{{ $t(\"creatorAI.dashboard.loading\") }}</p>\n      </div>\n      <div v-else-if=\"contents.length === 0\" class=\"empty-state\">\n        <p>{{ $t(\"creatorAI.dashboard.noContents\") || \"No content found\" }}</p>\n      </div>\n      <div\n        v-else\n        v-for=\"content in contents\"\n        :key=\"content.id\"\n        class=\"content-card\"\n        @click=\"viewContent(content.id)\"\n      >\n        <div class=\"card-header\">\n          <h3>{{ content.topic || \"Untitled Content\" }}</h3>\n          <div class=\"card-actions\">\n            <span class=\"content-type-badge\">\n              {{ content.contentType || \"Content\" }}\n            </span>\n            <button\n              class=\"delete-button\"\n              @click.stop=\"confirmDeleteContent($event, content.id)\"\n              title=\"Delete content\"\n            >\n              <i class=\"fa-solid fa-trash\"></i>\n            </button>\n          </div>\n        </div>\n        <p class=\"description\">\n          {{ truncateContent(content.generatedContent) || \"No content\" }}\n        </p>\n        <div class=\"content-meta\">\n          <span>{{ formatDate(content.createdAt) }}</span>\n          <span>{{\n            content.Project ? content.Project.title : \"No project\"\n          }}</span>\n        </div>\n        <div v-if=\"content.imageUrl\" class=\"content-thumbnail\">\n          <img\n            :src=\"ensureAbsoluteUrl(content.imageUrl)\"\n            alt=\"Content thumbnail\"\n          />\n        </div>\n      </div>\n    </div>\n\n    <!-- Project Action Menu Overlay (Re-created) -->\n    <div\n      v-if=\"showProjectMenu\"\n      class=\"project-menu-overlay\"\n      @click=\"closeProjectMenu\"\n    >\n      <div class=\"project-menu\" @click.stop>\n        <h3>Select Option</h3>\n        <div class=\"project-name\">{{ projectMenuTitle }}</div>\n        <div class=\"menu-options\">\n          <button\n            class=\"menu-button edit-button\"\n            @click=\"goToEditor(projectMenuId)\"\n          >\n            <i class=\"fas fa-edit\"></i> Edit Content\n          </button>\n          <button\n            class=\"menu-button view-button\"\n            @click=\"goToFinal(projectMenuId)\"\n          >\n            <i class=\"fas fa-eye\"></i> View Final Content\n          </button>\n          <button class=\"menu-button close-button\" @click=\"closeProjectMenu\">\n            <i class=\"fas fa-times\"></i> Cancel\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <!-- Content Action Menu Overlay -->\n    <div\n      v-if=\"showContentMenu\"\n      class=\"project-menu-overlay\"\n      @click=\"closeContentMenu\"\n    >\n      <div class=\"project-menu\" @click.stop>\n        <h3>Select Option</h3>\n        <div class=\"project-name\">{{ contentMenuTitle }}</div>\n        <div class=\"menu-options\">\n          <button\n            class=\"menu-button view-button\"\n            @click=\"goToContentFinal(contentMenuId)\"\n          >\n            <i class=\"fas fa-eye\"></i> View Content\n          </button>\n          <button class=\"menu-button close-button\" @click=\"closeContentMenu\">\n            <i class=\"fas fa-times\"></i> Cancel\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"goToHome\">\n        {{ $t(\"creatorAI.dashboard.backToHome\") }}\n      </button>\n      <button class=\"primary-button\" @click=\"createNewContent\">\n        {{ $t(\"creatorAI.dashboard.createNew\") }}\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"DashboardStep\",\n  data() {\n    return {\n      projects: [],\n      contents: [],\n      loading: true,\n      activeTab: \"projects\",\n      deleteConfirmation: {\n        show: false,\n        projectId: null,\n      },\n      toast: {\n        show: false,\n        message: \"\",\n        type: \"info\", // info, success, error\n        timeout: null,\n      },\n      // Thay thế projectMenu bằng các biến riêng biệt\n      showProjectMenu: false,\n      projectMenuId: null,\n      projectMenuTitle: \"\",\n      // New variables for content menu\n      showContentMenu: false,\n      contentMenuId: null,\n      contentMenuTitle: \"\",\n    };\n  },\n  created() {\n    this.fetchProjects();\n  },\n  methods: {\n    async fetchProjects() {\n      try {\n        this.loading = true;\n\n        const response = await axios.get(\"/api/projects\");\n\n        if (response.data.success) {\n          this.projects = response.data.data;\n        } else {\n          console.error(\"Failed to load projects\");\n          this.projects = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching projects:\", error);\n        this.projects = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    async fetchContents() {\n      try {\n        this.loading = true;\n\n        const response = await axios.get(\"/api/content/contents\");\n\n        if (response.data.success) {\n          this.contents = response.data.data;\n        } else {\n          console.error(\"Failed to load contents\");\n          this.contents = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching contents:\", error);\n        this.contents = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString(undefined, {\n        year: \"numeric\",\n        month: \"short\",\n        day: \"numeric\",\n      });\n    },\n\n    truncateContent(content) {\n      if (!content) return \"\";\n\n      // Remove markdown/HTML formatting\n      const plainText = content.replace(/#+\\s|<[^>]+>/g, \"\");\n\n      // Limit to ~100 characters\n      if (plainText.length <= 100) return plainText;\n      return plainText.substring(0, 97) + \"...\";\n    },\n\n    // Process image URL to ensure it's absolute\n    ensureAbsoluteUrl(url) {\n      if (!url) return null;\n\n      // Return as-is if already absolute\n      if (url.startsWith(\"http://\") || url.startsWith(\"https://\")) {\n        return url;\n      }\n\n      // For relative URLs starting with '/api'\n      if (url.startsWith(\"/api\")) {\n        return `${window.location.origin}${url}`;\n      }\n\n      // Default fallback - prefix with origin\n      return `${window.location.origin}${url}`;\n    },\n\n    viewProject(projectId) {\n      // Tìm project để hiển thị tên\n      const project = this.projects.find((p) => p.id === projectId);\n\n      // Hiển thị menu lựa chọn bằng các biến riêng biệt\n      this.showProjectMenu = true;\n      this.projectMenuId = projectId;\n      this.projectMenuTitle = project ? project.title : \"Project\";\n\n      console.log(\"Showing project menu for:\", projectId);\n    },\n\n    viewContent(contentId) {\n      // Find content to display name\n      const content = this.contents.find((c) => c.id === contentId);\n\n      // Show content menu\n      this.showContentMenu = true;\n      this.contentMenuId = contentId;\n      this.contentMenuTitle = content ? content.topic || \"Content\" : \"Content\";\n\n      console.log(\"Showing content menu for:\", contentId);\n    },\n\n    closeProjectMenu() {\n      this.showProjectMenu = false;\n    },\n\n    closeContentMenu() {\n      this.showContentMenu = false;\n    },\n\n    goToEditor(projectId) {\n      console.log(\"Going to editor for project:\", projectId);\n      this.$emit(\"edit-project\", projectId);\n      this.closeProjectMenu();\n    },\n\n    goToFinal(projectId) {\n      console.log(\"Going to final view for project:\", projectId);\n      this.$emit(\"view-final\", projectId);\n      this.closeProjectMenu();\n    },\n\n    goToContentFinal(contentId) {\n      console.log(\"Going to final view for content:\", contentId);\n      this.$emit(\"view-final\", contentId, \"content\");\n      this.closeContentMenu();\n    },\n\n    goToHome() {\n      this.$router.push(\"/\");\n    },\n\n    createNewContent() {\n      // Emit event to notify that we want to create new content with reset data\n      this.$emit(\"create-new\");\n    },\n\n    // Debug method to check project ID format\n    debugProjectId(projectId) {\n      console.log(\"Project ID value:\", projectId);\n      console.log(\"Project ID type:\", typeof projectId);\n      console.log(\"Project ID length:\", projectId ? projectId.length : \"N/A\");\n\n      // Check if ID is in UUID format (example check)\n      const uuidPattern =\n        /^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i;\n      console.log(\"Is UUID format:\", uuidPattern.test(projectId));\n\n      return projectId;\n    },\n\n    // Show toast message\n    showToast(message, type = \"info\") {\n      // Clear any existing timeout\n      if (this.toast.timeout) {\n        clearTimeout(this.toast.timeout);\n      }\n\n      // Set toast properties\n      this.toast.show = true;\n      this.toast.message = message;\n      this.toast.type = type;\n\n      // Auto hide after 3 seconds\n      this.toast.timeout = setTimeout(() => {\n        this.toast.show = false;\n      }, 3000);\n    },\n\n    confirmDeleteProject(event, projectId) {\n      // Ngăn chặn sự kiện click lan truyền đến card\n      event.stopPropagation();\n\n      // Debug project ID\n      this.debugProjectId(projectId);\n\n      if (confirm(this.$t(\"creatorAI.dashboard.deleteConfirm\"))) {\n        this.deleteProject(projectId);\n      }\n    },\n\n    confirmDeleteContent(event, contentId) {\n      // Prevent event propagation\n      event.stopPropagation();\n\n      if (\n        confirm(\n          \"Are you sure you want to delete this content? This cannot be undone.\"\n        )\n      ) {\n        this.deleteContent(contentId);\n      }\n    },\n\n    async deleteProject(projectId) {\n      try {\n        console.log(\"Deleting project with ID:\", projectId);\n\n        // Đảm bảo projectId không null và là string\n        if (!projectId) {\n          console.error(\"Project ID is null or undefined\");\n          this.showToast(this.$t(\"creatorAI.dashboard.deleteError\"), \"error\");\n          return;\n        }\n\n        // Log URL để debug\n        const url = `/api/projects/${projectId}`;\n        console.log(\"Delete URL:\", url);\n\n        const response = await axios({\n          method: \"DELETE\",\n          url: url,\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n        });\n\n        console.log(\"Delete response:\", response);\n\n        if (response.data && response.data.success) {\n          // Remove the project from the projects array\n          this.projects = this.projects.filter(\n            (project) => project.id !== projectId\n          );\n\n          // Show success message\n          this.showToast(\n            this.$t(\"creatorAI.dashboard.deleteSuccess\"),\n            \"success\"\n          );\n\n          // Refresh projects list\n          this.fetchProjects();\n        } else {\n          console.error(\"Failed to delete project\", response.data);\n          this.showToast(this.$t(\"creatorAI.dashboard.deleteFailed\"), \"error\");\n        }\n      } catch (error) {\n        console.error(\"Error deleting project:\", error);\n\n        // Log chi tiết lỗi để debug\n        if (error.response) {\n          console.error(\"Error response:\", error.response.data);\n          console.error(\"Error status:\", error.response.status);\n        }\n\n        this.showToast(\n          `${this.$t(\"creatorAI.dashboard.deleteError\")}: ${error.message}`,\n          \"error\"\n        );\n      }\n    },\n\n    async deleteContent(contentId) {\n      try {\n        console.log(\"Deleting content with ID:\", contentId);\n\n        if (!contentId) {\n          console.error(\"Content ID is null or undefined\");\n          this.showToast(\"Delete error: Invalid content ID\", \"error\");\n          return;\n        }\n\n        const url = `/api/content/contents/${contentId}`;\n        console.log(\"Delete URL:\", url);\n\n        const response = await axios({\n          method: \"DELETE\",\n          url: url,\n          headers: {\n            \"Conten