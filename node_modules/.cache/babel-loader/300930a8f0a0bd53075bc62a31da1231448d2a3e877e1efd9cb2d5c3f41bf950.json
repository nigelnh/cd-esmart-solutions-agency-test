{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"ImageStep\",\n  props: {\n    generatedImage: {\n      type: String,\n      default: null\n    },\n    imageStyles: {\n      type: Array,\n      required: true\n    },\n    initialStyle: {\n      type: String,\n      required: true\n    },\n    contentData: {\n      type: Object,\n      default: () => ({})\n    }\n  },\n  data() {\n    return {\n      selectedStyle: this.initialStyle,\n      isGenerating: false,\n      localImage: null,\n      error: null,\n      apiUrl: process.env.VUE_APP_API_URL || \"http://localhost:3001/api\"\n    };\n  },\n  computed: {\n    displayImage() {\n      // Nếu có ảnh được truyền từ props hoặc được tạo cục bộ\n      if (this.generatedImage) {\n        return this.generatedImage;\n      }\n      if (this.localImage) {\n        return `data:image/png;base64,${this.localImage}`;\n      }\n      return null;\n    }\n  },\n  methods: {\n    selectStyle(style) {\n      this.selectedStyle = style;\n      this.$emit(\"style-select\", style);\n    },\n    async generateImage() {\n      this.isGenerating = true;\n      this.error = null;\n      try {\n        // Kiểm tra xem có nội dung trong contentData không\n        if (this.contentData && this.contentData.content) {\n          // Có nội dung, sử dụng API generate-from-content\n          const response = await axios.post(`${this.apiUrl}/image/generate-from-content`, {\n            content: this.contentData.content,\n            style: this.selectedStyle\n          });\n          if (response.data.success) {\n            this.localImage = response.data.image;\n            this.$emit(\"image-generated\", {\n              image: response.data.image,\n              prompt: response.data.prompt,\n              style: response.data.style\n            });\n          } else {\n            this.error = response.data.error || \"Failed to generate image\";\n            console.error(\"Error generating image:\", this.error);\n          }\n        } else {\n          // Không có nội dung, sử dụng một prompt đơn giản dựa trên chủ đề\n          const topicPrompt = this.contentData.topic || \"Creative content\";\n          const response = await axios.post(`${this.apiUrl}/image/generate-from-topic`, {\n            topic: topicPrompt,\n            style: this.selectedStyle\n          });\n          if (response.data.success) {\n            this.localImage = response.data.image;\n            this.$emit(\"image-generated\", {\n              image: response.data.image,\n              prompt: response.data.prompt,\n              style: response.data.style\n            });\n          } else {\n            this.error = response.data.error || \"Failed to generate image\";\n            console.error(\"Error generating image:\", this.error);\n          }\n        }\n      } catch (error) {\n        console.error(\"Error generating image:\", error);\n        this.error = `Failed to generate image: ${error.message || \"Unknown error\"}`;\n\n        // Fallback for development/demo\n        if (error.message.includes(\"Network Error\") || error.message.includes(\"Connection refused\")) {\n          this.useFallbackImage();\n        }\n      } finally {\n        this.isGenerating = false;\n      }\n    },\n    // Sử dụng ảnh mẫu cho mục đích dev/demo khi API không khả dụng\n    useFallbackImage() {\n      console.log(\"Using fallback image with style:\", this.selectedStyle);\n\n      // Placeholder URLs dựa trên style\n      const placeholderUrls = {\n        Realistic: \"https://placehold.co/600x400/1c1c4c/white?text=Realistic+Style\",\n        \"3D\": \"https://placehold.co/600x400/2a2a6c/white?text=3D+Style\",\n        Minimalist: \"https://placehold.co/600x400/3a3a8c/white?text=Minimalist+Style\",\n        Watercolor: \"https://placehold.co/600x400/4a4aac/white?text=Watercolor+Style\",\n        \"Flat Vector\": \"https://placehold.co/600x400/5a5acc/white?text=Flat+Vector+Style\"\n      };\n\n      // Tạo ảnh local URL\n      const placeholderUrl = placeholderUrls[this.selectedStyle] || placeholderUrls[\"Realistic\"];\n\n      // Gửi sự kiện và cập nhật localImage\n      this.$emit(\"image-generated\", {\n        image: placeholderUrl,\n        prompt: `Fallback image in ${this.selectedStyle} style`,\n        style: this.selectedStyle\n      });\n\n      // Sử dụng URL trực tiếp thay vì base64\n      this.localImage = placeholderUrl;\n    }\n  },\n  mounted() {\n    // Tự động tạo ảnh khi component được mount (nếu không có ảnh sẵn)\n    if (!this.generatedImage && !this.isGenerating) {\n      this.generateImage();\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","generatedImage","type","String","default","imageStyles","Array","required","initialStyle","contentData","Object","data","selectedStyle","isGenerating","localImage","error","apiUrl","process","env","VUE_APP_API_URL","computed","displayImage","methods","selectStyle","style","$emit","generateImage","content","response","post","success","image","prompt","console","topicPrompt","topic","message","includes","useFallbackImage","log","placeholderUrls","Realistic","Minimalist","Watercolor","placeholderUrl","mounted"],"sources":["/Users/nhannguyen/Desktop/Duan/esmart-solutions-agency/src/components/creator-ai/ImageStep.vue"],"sourcesContent":["<template>\n  <div class=\"step-content\">\n    <h2>Image Generation</h2>\n    <div class=\"image-container\">\n      <div class=\"generated-image\">\n        <img v-if=\"displayImage\" :src=\"displayImage\" alt=\"AI Generated Image\" />\n        <div v-else class=\"image-placeholder\">\n          <div class=\"loading-spinner\"></div>\n          <p>\n            {{\n              isGenerating\n                ? \"Generating image...\"\n                : \"Select a style and click Generate\"\n            }}\n          </p>\n        </div>\n      </div>\n      <div class=\"style-options\">\n        <h3>Image Style</h3>\n        <div class=\"style-buttons\">\n          <button\n            v-for=\"style in imageStyles\"\n            :key=\"style\"\n            :class=\"['style-button', { active: selectedStyle === style }]\"\n            @click=\"selectStyle(style)\"\n          >\n            {{ style }}\n          </button>\n        </div>\n        <div class=\"generate-button-container\">\n          <button\n            class=\"generate-button\"\n            @click=\"generateImage\"\n            :disabled=\"isGenerating\"\n          >\n            {{ isGenerating ? \"Generating...\" : \"Generate Image\" }}\n          </button>\n        </div>\n      </div>\n    </div>\n    <div v-if=\"error\" class=\"error-message\">\n      {{ error }}\n    </div>\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"$emit('prev')\">Back</button>\n      <button\n        class=\"primary-button\"\n        :disabled=\"!displayImage\"\n        @click=\"$emit('next')\"\n      >\n        Finalize\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ImageStep\",\n  props: {\n    generatedImage: {\n      type: String,\n      default: null,\n    },\n    imageStyles: {\n      type: Array,\n      required: true,\n    },\n    initialStyle: {\n      type: String,\n      required: true,\n    },\n    contentData: {\n      type: Object,\n      default: () => ({}),\n    },\n  },\n  data() {\n    return {\n      selectedStyle: this.initialStyle,\n      isGenerating: false,\n      localImage: null,\n      error: null,\n      apiUrl: process.env.VUE_APP_API_URL || \"http://localhost:3001/api\",\n    };\n  },\n  computed: {\n    displayImage() {\n      // Nếu có ảnh được truyền từ props hoặc được tạo cục bộ\n      if (this.generatedImage) {\n        return this.generatedImage;\n      }\n\n      if (this.localImage) {\n        return `data:image/png;base64,${this.localImage}`;\n      }\n\n      return null;\n    },\n  },\n  methods: {\n    selectStyle(style) {\n      this.selectedStyle = style;\n      this.$emit(\"style-select\", style);\n    },\n    async generateImage() {\n      this.isGenerating = true;\n      this.error = null;\n\n      try {\n        // Kiểm tra xem có nội dung trong contentData không\n        if (this.contentData && this.contentData.content) {\n          // Có nội dung, sử dụng API generate-from-content\n          const response = await axios.post(\n            `${this.apiUrl}/image/generate-from-content`,\n            {\n              content: this.contentData.content,\n              style: this.selectedStyle,\n            }\n          );\n\n          if (response.data.success) {\n            this.localImage = response.data.image;\n            this.$emit(\"image-generated\", {\n              image: response.data.image,\n              prompt: response.data.prompt,\n              style: response.data.style,\n            });\n          } else {\n            this.error = response.data.error || \"Failed to generate image\";\n            console.error(\"Error generating image:\", this.error);\n          }\n        } else {\n          // Không có nội dung, sử dụng một prompt đơn giản dựa trên chủ đề\n          const topicPrompt = this.contentData.topic || \"Creative content\";\n\n          const response = await axios.post(\n            `${this.apiUrl}/image/generate-from-topic`,\n            {\n              topic: topicPrompt,\n              style: this.selectedStyle,\n            }\n          );\n\n          if (response.data.success) {\n            this.localImage = response.data.image;\n            this.$emit(\"image-generated\", {\n              image: response.data.image,\n              prompt: response.data.prompt,\n              style: response.data.style,\n            });\n          } else {\n            this.error = response.data.error || \"Failed to generate image\";\n            console.error(\"Error generating image:\", this.error);\n          }\n        }\n      } catch (error) {\n        console.error(\"Error generating image:\", error);\n        this.error = `Failed to generate image: ${\n          error.message || \"Unknown error\"\n        }`;\n\n        // Fallback for development/demo\n        if (\n          error.message.includes(\"Network Error\") ||\n          error.message.includes(\"Connection refused\")\n        ) {\n          this.useFallbackImage();\n        }\n      } finally {\n        this.isGenerating = false;\n      }\n    },\n\n    // Sử dụng ảnh mẫu cho mục đích dev/demo khi API không khả dụng\n    useFallbackImage() {\n      console.log(\"Using fallback image with style:\", this.selectedStyle);\n\n      // Placeholder URLs dựa trên style\n      const placeholderUrls = {\n        Realistic:\n          \"https://placehold.co/600x400/1c1c4c/white?text=Realistic+Style\",\n        \"3D\": \"https://placehold.co/600x400/2a2a6c/white?text=3D+Style\",\n        Minimalist:\n          \"https://placehold.co/600x400/3a3a8c/white?text=Minimalist+Style\",\n        Watercolor:\n          \"https://placehold.co/600x400/4a4aac/white?text=Watercolor+Style\",\n        \"Flat Vector\":\n          \"https://placehold.co/600x400/5a5acc/white?text=Flat+Vector+Style\",\n      };\n\n      // Tạo ảnh local URL\n      const placeholderUrl =\n        placeholderUrls[this.selectedStyle] || placeholderUrls[\"Realistic\"];\n\n      // Gửi sự kiện và cập nhật localImage\n      this.$emit(\"image-generated\", {\n        image: placeholderUrl,\n        prompt: `Fallback image in ${this.selectedStyle} style`,\n        style: this.selectedStyle,\n      });\n\n      // Sử dụng URL trực tiếp thay vì base64\n      this.localImage = placeholderUrl;\n    },\n  },\n  mounted() {\n    // Tự động tạo ảnh khi component được mount (nếu không có ảnh sẵn)\n    if (!this.generatedImage && !this.isGenerating) {\n      this.generateImage();\n    }\n  },\n};\n</script>\n\n<style scoped>\n.step-content {\n  background: white;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n  color: #1c1c4c;\n  margin-bottom: 2rem;\n  text-align: center;\n}\n\n.image-container {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 2rem;\n}\n\n.generated-image {\n  background: #f8f9fa;\n  border-radius: 8px;\n  padding: 1rem;\n  min-height: 300px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.generated-image img {\n  max-width: 100%;\n  max-height: 300px;\n  object-fit: contain;\n  border-radius: 4px;\n}\n\n.image-placeholder {\n  width: 100%;\n  height: 300px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  background: #f1f1f4;\n  border-radius: 4px;\n  color: #1c1c4c;\n}\n\n.style-options {\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n}\n\n.style-buttons {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));\n  gap: 1rem;\n  margin-bottom: 1.5rem;\n}\n\n.style-button {\n  padding: 0.75rem;\n  border: 1px solid #ddd;\n  border-radius: 6px;\n  background: white;\n  cursor: pointer;\n  transition: all 0.3s ease;\n}\n\n.style-button.active {\n  background: #1c1c4c;\n  color: white;\n  border-color: #1c1c4c;\n}\n\n.generate-button-container {\n  margin-top: 1.5rem;\n  text-align: center;\n}\n\n.generate-button {\n  background: #1c1c4c;\n  color: white;\n  border: none;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: background-color 0.3s ease;\n  width: 100%;\n}\n\n.generate-button:hover:not(:disabled) {\n  background: #2a2a6c;\n}\n\n.generate-button:disabled {\n  background: #7a7a9c;\n  cursor: not-allowed;\n}\n\n.button-group {\n  display: flex;\n  gap: 1rem;\n  justify-content: center;\n  margin-top: 2rem;\n}\n\n.primary-button {\n  background: #1c1c4c;\n  color: white;\n  border: none;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: background-color 0.3s ease;\n}\n\n.primary-button:hover:not(:disabled) {\n  background: #2a2a6c;\n}\n\n.primary-button:disabled {\n  background: #7a7a9c;\n  cursor: not-allowed;\n}\n\n.secondary-button {\n  background: white;\n  color: #1c1c4c;\n  border: 1px solid #1c1c4c;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: all 0.3s ease;\n}\n\n.secondary-button:hover {\n  background: #f8f9fa;\n}\n\n.loading-spinner {\n  display: inline-block;\n  width: 40px;\n  height: 40px;\n  border: 3px solid rgba(28, 28, 76, 0.3);\n  border-radius: 50%;\n  border-top-color: #1c1c4c;\n  animation: spin 1s linear infinite;\n  margin-bottom: 1rem;\n}\n\n.error-message {\n  margin-top: 1rem;\n  padding: 0.75rem;\n  background-color: #fff0f0;\n  border-left: 4px solid #ff5252;\n  color: #d32f2f;\n  border-radius: 4px;\n}\n\n@keyframes spin {\n  to {\n    transform: rotate(360deg);\n  }\n}\n\n@media (max-width: 768px) {\n  .image-container {\n    grid-template-columns: 1fr;\n  }\n\n  .button-group {\n    flex-direction: column;\n  }\n\n  .primary-button,\n  .secondary-button {\n    width: 100%;\n  }\n}\n</style>\n"],"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,cAAc,EAAE;MACdC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,WAAW,EAAE;MACXH,IAAI,EAAEI,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,YAAY,EAAE;MACZN,IAAI,EAAEC,MAAM;MACZI,QAAQ,EAAE;IACZ,CAAC;IACDE,WAAW,EAAE;MACXP,IAAI,EAAEQ,MAAM;MACZN,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IACpB;EACF,CAAC;EACDO,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,IAAI,CAACJ,YAAY;MAChCK,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,IAAI;MAChBC,KAAK,EAAE,IAAI;MACXC,MAAM,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAc,IAAK;IACzC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb;MACA,IAAI,IAAI,CAACpB,cAAc,EAAE;QACvB,OAAO,IAAI,CAACA,cAAc;MAC5B;MAEA,IAAI,IAAI,CAACa,UAAU,EAAE;QACnB,OAAO,yBAAyB,IAAI,CAACA,UAAU,EAAE;MACnD;MAEA,OAAO,IAAI;IACb;EACF,CAAC;EACDQ,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,CAACZ,aAAY,