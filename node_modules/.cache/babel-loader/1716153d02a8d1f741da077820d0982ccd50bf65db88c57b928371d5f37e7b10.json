 Tab \"), $data.activeTab === 'projects' ? (_openBlock(), _createElementBlock(\"div\", _hoisted_3, [$data.loading ? (_openBlock(), _createElementBlock(\"div\", _hoisted_4, [..._hoisted_6])) : $data.projects.length === 0 ? (_openBlock(), _createElementBlock(\"div\", _hoisted_7, [..._hoisted_9])) : (_openBlock(true), _createElementBlock(_Fragment, {\n    key: 2\n  }, _renderList($data.projects, project => {\n    return _openBlock(), _createElementBlock(\"div\", {\n      key: project.id,\n      class: _normalizeClass([\"content-card\", {\n        finished: project.status === 'finished'\n      }]),\n      onClick: $event => $options.viewProject(project.id)\n    }, [_createElementVNode(\"div\", _hoisted_11, [_createElementVNode(\"h3\", null, _toDisplayString(project.title), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_12, [_createElementVNode(\"span\", {\n      class: _normalizeClass([\"status-badge\", project.status])\n    }, _toDisplayString(project.status === \"in_progress\" ? \"In Progress\" : \"Finished\"), 3 /* TEXT, CLASS */), _createElementVNode(\"button\", {\n      class: \"delete-button\",\n      onClick: _withModifiers($event => $options.confirmDeleteProject(project.id), [\"stop\"]),\n      title: \"Delete project\"\n    }, [..._hoisted_15], 8 /* PROPS */, _hoisted_13)])]), _createElementVNode(\"p\", _hoisted_16, _toDisplayString(project.description || \"No description\"), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_17, [_createElementVNode(\"span\", null, _toDisplayString($options.formatDate(project.createdAt)), 1 /* TEXT */), _createElementVNode(\"span\", null, _toDisplayString(project.type), 1 /* TEXT */)])], 10 /* CLASS, PROPS */, _hoisted_10);\n  }), 128 /* KEYED_FRAGMENT */))])) : _createCommentVNode(\"v-if\", true), _createElementVNode(\"div\", _hoisted_18, [_createElementVNode(\"button\", {\n    class: \"secondary-button\",\n    onClick: _cache[0] || (_cache[0] = (...args) => $options.goToHome && $options.goToHome(...args))\n  }, \"Bac