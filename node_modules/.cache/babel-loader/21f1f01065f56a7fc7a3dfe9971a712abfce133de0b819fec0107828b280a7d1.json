}}</h2>\n\n    <!-- Toast Message -->\n    <div v-if=\"toast.show\" class=\"toast-message\" :class=\"toast.type\">\n      <span>{{ toast.message }}</span>\n      <button @click=\"toast.show = false\" class=\"close-toast\">&times;</button>\n    </div>\n\n    <!-- Tab Navigation -->\n    <div class=\"tab-navigation\">\n      <button\n        class=\"tab-button\"\n        :class=\"{ active: activeTab === 'projects' }\"\n        @click=\"activeTab = 'projects'\"\n      >\n        {{ $t(\"creatorAI.dashboard.projectsTab\") || \"Projects\" }}\n      </button>\n      <button\n        class=\"tab-button\"\n        :class=\"{ active: activeTab === 'content' }\"\n        @click=\"\n          activeTab = 'content';\n          fetchContents();\n        \"\n      >\n        {{ $t(\"creatorAI.dashboard.contentsTab\") || \"Content\" }}\n      </button>\n    </div>\n\n    <!-- Projects Tab -->\n    <div v-if=\"activeTab === 'projects'\" class=\"content-grid\">\n      <div v-if=\"loading\" class=\"loading-indicator\">\n        <p>{{ $t(\"creatorAI.dashboard.loading\") }}</p>\n      </div>\n      <div v-else-if=\"projects.length === 0\" class=\"empty-state\">\n        <p>{{ $t(\"creatorAI.dashboard.noProjects\") }}</p>\n      </div>\n      <div\n        v-else\n        v-for=\"project in projects\"\n        :key=\"project.id\"\n        class=\"content-card\"\n        :class=\"{ finished: project.status === 'finished' }\"\n        @click=\"viewProject(project.id)\"\n      >\n        <div class=\"card-header\">\n          <h3>{{ project.title }}</h3>\n          <div class=\"card-actions\">\n            <span class=\"status-badge\" :class=\"project.status\">\n              {{\n                project.status === \"in_progress\"\n                  ? $t(\"creatorAI.dashboard.inProgress\")\n                  : $t(\"creatorAI.dashboard.finished\")\n              }}\n            </span>\n            <button\n              class=\"delete-button\"\n              @click.stop=\"confirmDeleteProject($event, project.id)\"\n              title=\"Delete project\"\n            >\n              <i class=\"fa-solid fa-trash\"></i>\n            </button>\n          </div>\n        </div>\n        <p class=\"description\">\n          {{ project.description || $t(\"creatorAI.dashboard.noDescription\") }}\n        </p>\n        <div class=\"content-meta\">\n          <span>{{ formatDate(project.createdAt) }}</span>\n          <span>{{ project.type }}</span>\n        </div>\n      </div>\n    </div>\n\n    <!-- Contents Tab -->\n    <div v-if=\"activeTab === 'content'\" class=\"content-grid\">\n      <div v-if=\"loading\" class=\"loading-indicator\">\n        <p>{{ $t(\"creatorAI.dashboard.loading\") }}</p>\n      </div>\n      <div v-else-if=\"contents.length === 0\" class=\"empty-state\">\n        <p>{{ $t(\"creatorAI.dashboard.noContents\") || \"No content found\" }}</p>\n      </div>\n      <div\n        v-else\n        v-for=\"content in contents\"\n        :key=\"content.id\"\n        class=\"content-card\"\n        @click=\"viewContent(content.id)\"\n      >\n        <div class=\"card-header\">\n          <h3>{{ content.topic || \"Untitled Content\" }}</h3>\n          <div class=\"card-actions\">\n            <span class=\"content-type-badge\">\n              {{ content.contentType || \"Content\" }}\n            </span>\n            <button\n              class=\"delete-button\"\n              @click.stop=\"confirmDeleteContent($event, content.id)\"\n              title=\"Delete content\"\n            >\n              <i class=\"fa-solid fa-trash\"></i>\n            </button>\n          </div>\n        </div>\n        <p class=\"description\">\n          {{ truncateContent(content.generatedContent) || \"No content\" }}\n        </p>\n        <div class=\"content-meta\">\n          <span>{{ formatDate(content.createdAt) }}</span>\n          <span>{{\n            content.Project ? content.Project.title : \"No project\"\n          }}</span>\n        </div>\n        <div v-if=\"content.imageUrl\" class=\"content-thumbnail\">\n          <img\n            :src=\"ensureAbsoluteUrl(content.imageUrl)\"\n            alt=\"Content thumbnail\"\n          />\n        </div>\n      </div>\n    </div>\n\n    <!-- Project Action Menu Overlay (Re-created) -->\n    <div\n      v-if=\"showProjectMenu\"\n      class=\"project-menu-overlay\"\n      @click=\"closeProjectMenu\"\n    >\n      <div class=\"project-menu\" @click.stop>\n        <h3>Select Option</h3>\n        <div class=\"project-name\">{{ projectMenuTitle }}</div>\n        <div class=\"menu-options\">\n          <button\n            class=\"menu-button edit-button\"\n            @click=\"goToEditor(projectMenuId)\"\n          >\n            <i class=\"fas fa-edit\"></i> Edit Content\n          </button>\n          <button\n            class=\"menu-button view-button\"\n            @click=\"goToFinal(projectMenuId)\"\n          >\n            <i class=\"fas fa-eye\"></i> View Final Content\n          </button>\n          <button class=\"menu-button close-button\" @click=\"closeProjectMenu\">\n            <i class=\"fas fa-times\"></i> Cancel\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <!-- Content Action Menu Overlay -->\n    <div\n      v-if=\"showContentMenu\"\n      class=\"project-menu-overlay\"\n      @click=\"closeContentMenu\"\n    >\n      <div class=\"project-menu\" @click.stop>\n        <h3>Select Option</h3>\n        <div class=\"project-name\">{{ contentMenuTitle }}</div>\n        <div class=\"menu-options\">\n          <button\n            class=\"menu-button view-button\"\n            @click=\"goToContentFinal(contentMenuId)\"\n          >\n            <i class=\"fas fa-eye\"></i> View Content\n          </button>\n          <button class=\"menu-button close-button\" @click=\"closeContentMenu\">\n            <i class=\"fas fa-times\"></i> Cancel\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"goToHome\">\n        {{ $t(\"creatorAI.dashboard.backToHome\") }}\n      </button>\n      <button class=\"primary-button\" @click=\"createNewContent\">\n        {{ $t(\"creatorAI.dashboard.createNew\") }}\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"DashboardStep\",\n  data() {\n    return {\n      projects: [],\n      contents: [],\n      loading: true,\n      activeTab: \"projects\",\n      deleteConfirmation: {\n        show: false,\n        projectId: null,\n      },\n      toast: {\n        show: false,\n        message: \"\",\n        type: \"info\", // info, success, error\n        timeout: null,\n      },\n      // Thay thế projectMenu bằng các biến riêng biệt\n      showProjectMenu: false,\n      projectMenuId: null,\n      projectMenuTitle: \"\",\n      // New variables for content menu\n      showContentMenu: false,\n      contentMenuId: null,\n      contentMenuTitle: \"\",\n    };\n  },\n  created() {\n    this.fetchProjects();\n  },\n  methods: {\n    async fetchProjects() {\n      try {\n        this.loading = true;\n\n        const response = await axios.get(\"/api/projects\");\n\n        if (response.data.success) {\n          this.projects = response.data.data;\n        } else {\n          console.error(\"Failed to load projects\");\n          this.projects = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching projects:\", error);\n        this.projects = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    async fetchContents() {\n      try {\n        this.loading = true;\n\n        const response = await axios.get(\"/api/content/contents\");\n\n        if (response.data.success) {\n          this.contents = response.data.data;\n        } else {\n          console.error(\"Failed to load contents\");\n          this.contents = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching contents:\", error);\n        this.contents = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toLocaleDateString(undefined, {\n        year: \"numeric\",\n        month: \"short\",\n        day: \"numeric\",\n      });\n    },\n\n    truncateContent(content) {\n      if (!content) return \"\";\n\n      // Remove markdown/HTML formatting\n      const plainText = content.replace(/#+\\s|<[^>]+>/g, \"\");\n\n      // Limit to ~100 characters\n      if (plainText.length <= 100) return plainText;\n      return plainText.substring(0, 97) + \"...\";\n    },\n\n    // Process image URL to ensure it's absolute\n    ensureAbsoluteUrl(url) {\n      if (!url) return null;\n\n      // Return as-is if already absolute\n      if (url.startsWith(\"http://\") || url.startsWith(\"https://\")) {\n        return url;\n      }\n\n      // For relative URLs starting with '/api'\n      if (url.startsWith(\"/api\")) {\n        return `${window.location.origin}${url}`;\n      }\n\n      // Default fallback - prefix with origin\n      return `${window.location.origin}${url}`;\n    },\n\n    viewProject(projectId) {\n      // Tìm project để hiển thị tên\n      const project = this.projects.find((p) => p.id === projectId);\n\n      // Hiển thị menu lựa chọn bằng các biến riêng biệt\n      this.showProjectMenu = true;\n      this.projectMenuId = projectId;\n      this.projectMenuTitle = project ? project.title : \"Project\";\n\n      console.log(\"Showing project menu for:\", projectId);\n    },\n\n    viewContent(contentId) {\n      // Find content to display name\n      const content = this.contents.find((c) => c.id === contentId);\n\n      // Show content menu\n      this.showContentMenu = true;\n      this.contentMenuId = contentId;\n      this.contentMenuTitle = content ? content.topic || \"Content\" : \"Content\";\n\n      console.log(\"S