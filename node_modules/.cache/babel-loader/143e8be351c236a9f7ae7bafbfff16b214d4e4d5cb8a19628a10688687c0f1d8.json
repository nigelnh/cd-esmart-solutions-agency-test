{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"RequirementsStep\",\n  props: {\n    initialData: {\n      type: Object,\n      required: true\n    }\n  },\n  data() {\n    return {\n      formData: {\n        ...this.initialData\n      },\n      isLoading: false,\n      apiUrl: \"http://localhost:3001/api/generate-content\"\n    };\n  },\n  methods: {\n    async generateContent() {\n      try {\n        // Validate input\n        if (!this.formData.topic || !this.formData.keywords) {\n          alert(\"Please enter both topic and keywords\");\n          return;\n        }\n        this.isLoading = true;\n\n        // Call backend API\n        const response = await axios.post(this.apiUrl, this.formData);\n        if (response.data.success) {\n          // Update the content data with the generated content\n          const updatedData = {\n            ...this.formData,\n            content: response.data.content\n          };\n\n          // Emit update event with new data\n          this.$emit(\"update:data\", updatedData);\n\n          // Go to next step\n          this.$emit(\"next\");\n        } else {\n          throw new Error(response.data.error || \"Unknown error occurred\");\n        }\n      } catch (error) {\n        console.error(\"Error generating content:\", error);\n        alert(\"Failed to generate content: \" + (error.message || \"Unknown error\"));\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","initialData","type","Object","required","data","formData","isLoading","apiUrl","methods","generateContent","topic","keywords","alert","response","post","success","updatedData","content","$emit","Error","error","console","message"],"sources":["/Users/nhannguyen/Desktop/Duan/esmart-solutions-agency/src/components/creator-ai/RequirementsStep.vue"],"sourcesContent":["<template>\n  <div class=\"step-content\">\n    <h2>AI Content Requirements</h2>\n    <div class=\"form-container\">\n      <div class=\"form-group\">\n        <label>Topic</label>\n        <input\n          v-model=\"formData.topic\"\n          type=\"text\"\n          placeholder=\"Enter your topic\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label>Keywords</label>\n        <input\n          v-model=\"formData.keywords\"\n          type=\"text\"\n          placeholder=\"Enter keywords (comma separated)\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label>Tone</label>\n        <select v-model=\"formData.tone\">\n          <option value=\"professional\">Professional</option>\n          <option value=\"casual\">Casual</option>\n          <option value=\"friendly\">Friendly</option>\n          <option value=\"formal\">Formal</option>\n        </select>\n      </div>\n      <div class=\"form-group\">\n        <label>Content Type</label>\n        <select v-model=\"formData.type\">\n          <option value=\"blog\">Blog Post</option>\n          <option value=\"social\">Social Media Post</option>\n          <option value=\"landing\">Landing Page</option>\n          <option value=\"email\">Email Marketing</option>\n        </select>\n      </div>\n    </div>\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"$emit('prev')\">Back</button>\n      <button\n        class=\"primary-button\"\n        @click=\"generateContent\"\n        :disabled=\"isLoading\"\n      >\n        <span v-if=\"isLoading\" class=\"loading-spinner\"></span>\n        {{ isLoading ? \"Generating...\" : \"Create Content\" }}\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n  \n\nexport default {\n  name: \"RequirementsStep\",\n  props: {\n    initialData: {\n      type: Object,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      formData: { ...this.initialData },\n      isLoading: false,\n      apiUrl: \"http://localhost:3001/api/generate-content\",\n    };\n  },\n  methods: {\n    async generateContent() {\n      try {\n        // Validate input\n        if (!this.formData.topic || !this.formData.keywords) {\n          alert(\"Please enter both topic and keywords\");\n          return;\n        }\n\n        this.isLoading = true;\n\n        // Call backend API\n        const response = await axios.post(this.apiUrl, this.formData);\n\n        if (response.data.success) {\n          // Update the content data with the generated content\n          const updatedData = {\n            ...this.formData,\n            content: response.data.content,\n          };\n\n          // Emit update event with new data\n          this.$emit(\"update:data\", updatedData);\n\n          // Go to next step\n          this.$emit(\"next\");\n        } else {\n          throw new Error(response.data.error || \"Unknown error occurred\");\n        }\n      } catch (error) {\n        console.error(\"Error generating content:\", error);\n        alert(\n          \"Failed to generate content: \" + (error.message || \"Unknown error\")\n        );\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.step-content {\n  background: white;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n  color: #1c1c4c;\n  margin-bottom: 2rem;\n  text-align: center;\n}\n\n.form-container {\n  display: grid;\n  gap: 1.5rem;\n  max-width: 600px;\n  margin: 0 auto;\n}\n\n.form-group {\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n\n.form-group label {\n  font-weight: 500;\n  color: #1c1c4c;\n}\n\n.form-group input,\n.form-group select {\n  padding: 0.75rem;\n  border: 1px solid #ddd;\n  border-radius: 6px;\n  font-size: 1rem;\n}\n\n.button-group {\n  display: flex;\n  gap: 1rem;\n  justify-content: center;\n  margin-top: 2rem;\n}\n\n.primary-button {\n  background: #1c1c4c;\n  color: white;\n  border: none;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: background-color 0.3s ease;\n  position: relative;\n}\n\n.primary-button:hover:not(:disabled) {\n  background: #2a2a6c;\n}\n\n.primary-button:disabled {\n  background: #7a7a9c;\n  cursor: not-allowed;\n}\n\n.secondary-button {\n  background: white;\n  color: #1c1c4c;\n  border: 1px solid #1c1c4c;\n  padding: 0.75rem 1.5rem;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: all 0.3s ease;\n}\n\n.secondary-button:hover {\n  background: #f8f9fa;\n}\n\n.loading-spinner {\n  display: inline-block;\n  width: 16px;\n  height: 16px;\n  border: 2px solid #ffffff;\n  border-radius: 50%;\n  border-top-color: transparent;\n  animation: spin 1s linear infinite;\n  margin-right: 8px;\n}\n\n@keyframes spin {\n  to {\n    transform: rotate(360deg);\n  }\n}\n\n@media (max-width: 768px) {\n  .button-group {\n    flex-direction: column;\n  }\n\n  .primary-button,\n  .secondary-button {\n    width: 100%;\n  }\n}\n</style>\n"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AAGzB,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE;IACLC,WAAW,EAAE;MACXC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QAAE,GAAG,IAAI,CAACL;MAAY,CAAC;MACjCM,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF;QACA,IAAI,CAAC,IAAI,CAACJ,QAAQ,CAACK,KAAI,IAAK,CAAC,IAAI,CAACL,QAAQ,CAACM,QAAQ,EAAE;UACnDC,KAAK,CAAC,sCAAsC,CAAC;UAC7C;QACF;QAEA,IAAI,CAACN,SAAQ,GAAI,IAAI;;QAErB;QACA,MAAMO,QAAO,GAAI,MAAMhB,KAAK,CAACiB,IAAI,CAAC,IAAI,CAACP,MAAM,EAAE,IAAI,CAACF,QAAQ,CAAC;QAE7D,IAAIQ,QAAQ,CAACT,IAAI,CAACW,OAAO,EAAE;UACzB;UACA,MAAMC,WAAU,GAAI;YAClB,GAAG,IAAI,CAACX,QAAQ;YAChBY,OAAO,EAAEJ,QAAQ,CAACT,IAAI,CAACa;UACzB,CAAC;;UAED;UACA,IAAI,CAACC,KAAK,CAAC,aAAa,EAAEF,WAAW,CAAC;;UAEtC;UACA,IAAI,CAACE,KAAK,CAAC,MAAM,CAAC;QACpB,OAAO;UACL,MAAM,IAAIC,KAAK,CAACN,QAAQ,CAACT,IAAI,CAACgB,KAAI,IAAK,wBAAwB,CAAC;QAClE;MACF,EAAE,OAAOA,KAAK,E