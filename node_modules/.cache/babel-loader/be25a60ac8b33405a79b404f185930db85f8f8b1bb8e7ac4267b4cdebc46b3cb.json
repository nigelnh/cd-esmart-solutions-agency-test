ted_1","_hoisted_2","_hoisted_3","_hoisted_4","_cache","$event","$data","content","placeholder","onInput","args","$options","updateContent","_hoisted_5","$props","contentId","onClick","analyzeSEO","_hoisted_6","_createTextVNode","_createCommentVNode","seoData","seoScore","optimizeSEO","_hoisted_7","isOptimizing","autoOptimizeSEO","_hoisted_8","_hoisted_9","_hoisted_10","_toDisplayString","optimizationProgress","_hoisted_11","_hoisted_12","_hoisted_13","_hoisted_14","_normalizeClass","getSEOScoreClass","_hoisted_15","keywordDensity","_hoisted_16","_hoisted_17","_hoisted_18","parseFloat","toFixed","readabilityScore","_hoisted_19","_hoisted_20","_hoisted_21","_hoisted_22","_hoisted_24","_hoisted_25","_hoisted_26","_Fragment","_renderList","keywordsList","keyword","index","suggestedKeywords","length","_hoisted_27","_hoisted_28","_hoisted_29","_hoisted_30","_hoisted_31","_hoisted_32","wordCount","detailedAnalysis","_hoisted_33","_hoisted_34","category","formatCategoryName","score","improvements","_hoisted_35","_hoisted_36","slice","improvement","i","_hoisted_37","goBack","saveContent","_ctx","$emit"],"sources":["/Users/nhannguyen/Desktop/Duan/esmart-solutions-agency/src/components/creator-ai/EditorStep.vue"],"sourcesContent":["<template>\n  <div class=\"step-content\">\n    <h2>Content Editor</h2>\n    <div class=\"editor-container\">\n      <div class=\"main-content\">\n        <textarea\n          v-model=\"content\"\n          placeholder=\"Your AI-generated content will appear here...\"\n          @input=\"updateContent\"\n        ></textarea>\n        \n        <div class=\"editor-actions\">\n          <button class=\"action-button seo-analyze\" @click=\"analyzeSEO\" v-if=\"contentId\">\n            <i class=\"fas fa-search\"></i> Analyze SEO\n          </button>\n          <button class=\"action-button seo-optimize\" @click=\"optimizeSEO\" v-if=\"seoData && seoData.seoScore < 80 && contentId\">\n            <i class=\"fas fa-magic\"></i> Optimize Content\n          </button>\n          <button class=\"action-button seo-auto\" @click=\"autoOptimizeSEO\" v-if=\"contentId && !isOptimizing\">\n            <i class=\"fas fa-robot\"></i> Auto Optimize\n          </button>\n          <div v-if=\"isOptimizing\" class=\"optimizing-indicator\">\n            <span class=\"spinner\"></span> Optimizing... ({{ optimizationProgress }})\n          </div>\n        </div>\n      </div>\n      <div class=\"sidebar\">\n        <div class=\"sidebar-section\">\n          <h3>SEO Score</h3>\n          <div class=\"seo-score-container\" v-if=\"seoData\">\n            <div class=\"score-circle\" :class=\"getSEOScoreClass()\">\n              {{ seoData.seoScore || 0 }}/100\n            </div>\n            <div class=\"score-details\">\n              <div v-if=\"seoData.keywordDensity\" class=\"detail-item\">\n                <span class=\"detail-label\">Keyword Density:</span>\n                <span class=\"detail-value\">{{ parseFloat(seoData.keywordDensity).toFixed(1) }}%</span>\n              </div>\n              <div v-if=\"seoData.readabilityScore\" class=\"detail-item\">\n                <span class=\"detail-label\">Readability:</span>\n                <span class=\"detail-value\">{{ seoData.readabilityScore }}/100</span>\n              </div>\n            </div>\n          </div>\n          <div v-else class=\"empty-seo-data\">\n            <p>Click \"Analyze SEO\" to check your content.</p>\n          </div>\n        </div>\n        \n        <div class=\"sidebar-section\">\n          <h3>SEO Keywords</h3>\n          <ul>\n            <li v-for=\"(keyword, index) in keywordsList\" :key=\"index\">\n              {{ keyword }}\n            </li>\n          </ul>\n          <div v-if=\"seoData && seoData.suggestedKeywords && seoData.suggestedKeywords.length > 0\">\n            <h4>Suggested Keywords</h4>\n            <ul class=\"suggested-keywords\">\n              <li v-for=\"(keyword, index) in seoData.suggestedKeywords\" :key=\"`sug-${index}`\">\n                {{ keyword }}\n              </li>\n            </ul>\n          </div>\n        </div>\n        \n        <div class=\"sidebar-section\">\n          <h3>Content Length</h3>\n          <div class=\"length\">{{ wordCount }} words</div>\n        </div>\n        \n        <div v-if=\"seoData && seoData.detailedAnalysis\" class=\"sidebar-section seo-analysis\">\n          <h3>SEO Analysis</h3>\n          <div v-for=\"(category, key) in seoData.detailedAnalysis\" :key=\"key\" class=\"analysis-category\">\n            <h4>{{ formatCategoryName(key) }} ({{ category.score }}/100)</h4>\n            <div class=\"improvement-list\" v-if=\"category.improvements\">\n              <p class=\"improvement-title\">Improvement Suggestions:</p>\n              <ul>\n                <li v-for=\"(improvement, i) in category.improvements.slice(0, 3)\" :key=\"i\">\n                  {{ improvement }}\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"button-group\">\n      <button class=\"secondary-button\" @click=\"goBack\">Back</button>\n      <button class=\"primary-button-save\" @click=\"saveContent\" v-if=\"contentId\">\n        Save Changes\n      </button>\n      <button class=\"primary-button\" @click=\"$emit('next')\">\n        Generate Image\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"EditorStep\",\n  props: {\n    initialContent: {\n      type: String,\n      default: \"\",\n    },\n    suggestedKeywords: {\n      type: Array,\n      default: () => [],\n    },\n    readabilityScore: {\n      type: Number,\n      default: 0,\n    },\n    contentLength: {\n      type: Number,\n      default: 0,\n    },\n    contentId: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      content: this.initialContent || \"\",\n      seoData: null,\n      isAnalyzing: false,\n      isOptimizing: false,\n      optimizationProgress: \"0/3\",\n      optimizationInterval: null\n    };\n  },\n  computed: {\n    keywordsList() {\n      return this.suggestedKeywords || [];\n    },\n    wordCount() {\n      if (!this.content) return 0;\n      return this.content.trim().split(/\\s+/).length;\n    },\n  },\n  methods: {\n    updateContent() {\n      if (this.content !== null && this.content !== undefined) {\n        this.$emit(\"update:content\", this.content);\n      }\n    },\n    goBack() {\n      if (this.contentId) {\n        this.$emit('go-to-dashboard');\n      } else {\n        this.$emit('prev');\n      }\n    },\n    async saveContent() {\n      try {\n        if (!this.contentId) {\n          console.error(\"Cannot save: No content ID provided\");\n          return;\n        }\n        \n        const response = await axios.put(`/api/content/contents/${this.contentId}`, {\n          generatedContent: this.content\n        });\n        \n        if (response.data.success) {\n          alert(\"Content saved successfully!\");\n        } else {\n          alert(\"Failed to save content: \" + response.data.error);\n        }\n      } catch (error) {\n        console.error(\"Error saving content:\", error);\n        alert(\"Error saving content: \" + error.message);\n      }\n    },\n    async analyzeSEO() {\n      if (!this.contentId) {\n        alert(\"Please save your content first\");\n        return;\n      }\n      \n      try {\n        this.isAnalyzing = true;\n        \n        // Gọi API phân tích SEO\n        const response = await axios.post(`/api/seo/analyze/${this.contentId}`);\n        \n        if (response.data.success) {\n          this.seoData = response.data.data.analysis;\n          \n          // Hiển thị thông báo thành công\n          this.$notify({\n            type: 'success',\n            title: 'SEO Analysis',\n            text: `SEO Score: ${this.seoData.score}/100`\n          });\n        } else {\n          alert(\"Failed to analyze SEO: \" + response.data.error);\n        }\n      } catch (error) {\n        console.error(\"Error analyzing SEO:\", error);\n        alert(\"Error analyzing SEO: \" + error.message);\n      } finally {\n        this.isAnalyzing = false;\n      }\n    },\n    async optimizeSEO() {\n      if (!this.contentId) {\n        alert(\"Please save your content first\");\n        return;\n      }\n      \n      try {\n        // Gọi API tối ưu hóa SEO\n        const response = await axios.post(`/api/seo/optimize/${this.contentId}`);\n        \n        if (response.data.success) {\n          const result = response.data.data;\n          \n          if (result.optimized) {\n            // Cập nhật nội dung và điểm số\n            this.content = result.content.generatedContent;\n            this.seoData = await this.fetchSEOData();\n            \n            // Hiển thị thông báo thành công\n            this.$notify({\n              type: 'success',\n              title: 'SEO Optimization',\n              text: `Content optimized! SEO Score: ${result.previousScore} → ${result.newScore}`\n            });\n          } else {\n            // Hiển thị thông báo nếu không thể tối ưu thêm\n            this.$notify({\n              type: 'info',\n              title: 'SEO Optimization',\n              text: result.message\n            });\n          }\n        } else {\n          alert(\"Failed to optimize SEO: \" + response.data.error);\n        }\n      } catch (error) {\n        console.error(\"Error optimizing SEO:\", error);\n        alert(\"Error optimizing SEO: \" + error.message);\n      }\n    },\n    async autoOptimizeSEO() {\n      if (!this.contentId) {\n        alert(\"Please save your content first\");\n        return;\n      }\n      \n      try {\n        this.isOptimizing = true;\n        this.optimizationProgress = \"0/3\";\n        \n        // Thiết lập timer để cập nhật trạng thái\n        let count = 0;\n        this.optimizationInterval = setInterval(() => {\n          count++;\n          this.optimizationProgress = `${Math.min(count, 3)}/3`;\n        }, 5000);\n        \n        // Gọi API tự động tối ưu hóa\n        const response = await axios.post(`/api/seo/auto-optimize/${this.contentId}`, {\n          maxOptimizations: 3,\n          targetScore: 80\n        });\n        \n        clearInterval(this.optimizationInterval);\n        \n        if (response.data.success) {\n          const result = response.data.data;\n          \n          // Cập nhật nội dung và điểm số\n          this.content = result.finalReport.content;\n          this.seoData = {\n            seoScore: result.finalScore,\n            keywordDensity: result.finalReport.keywordDensity,\n            readabilityScore: result.finalReport.readabilityScore,\n            suggestedKeywords: result.finalReport.suggestedKeywords,\n            detailedAnalysis: result.finalReport.detailedAnalysis\n          };\n          \n          // Hiển thị thông báo thành công\n          this.$notify({\n            type: 'success',\n            title: 'Auto SEO Optimization',\n            text: `Optimization completed! SEO Score: ${result.initialScore} → ${result.finalScore}`\n          });\n        } else {\n          alert(\"Failed to auto optimize SEO: \" + response.data.error);\n        }\n      } catch (error) {\n        console.error(\"Error auto optimizing SEO:\", error);\n        alert(\"Error auto optimizing SEO: \" + error.message);\n      } finally {\n        clearInterval(this.optimizationInterval);\n        this.isOptimizing = false;\n      }\n    },\n    async fetchSEOData() {\n      try {\n        const response = await axios.get(`/api/seo/report/${this.contentId}`);\n        \n        if (response.data.success) {\n          return {\n            seoScore: response.data.data.seoScore,\n            keywordDensity: response.data.data.keywordDensity,\n            readabilityScore: response.data.data.readabilityScore,\n            suggestedKeywords: response.data.data.suggestedKeywords,\n            detailedAnalysis: response.data.data.detailedAnalysis\n          };\n        }\n        return null;\n      } catch (error) {\n        console.error(\"Error fetching SEO data:\", error);\n        return null;\n      }\n    },\n    getSEOScoreClass() {\n      if (!this.seoData) return 'poor';\n      \n      const score = this.seoData.seoScore;\n      if (score >= 80) return 'excellent';\n      if (score >= 60) return 'good';\n      if (score >= 40) return 'average';\n      return 'poor';\n    },\n    formatCategoryName(key) {\n      return key\n        .replace(/([A-Z])/g, ' $1')\n        .replace(/^./, str => str.toUpperCase());\n    }\n  },\n  watch: {\n    initialContent(newValue) {\n      this.content = newValue || \"\";\n    },\n    contentId: {\n      immediate: true,\n      async handler(newValue) {\n        if (newValue) {\n          // Nếu có contentId, kiểm tra xem đã có dữ liệu SEO chưa\n          const seoData = await this.fetchSEOData();\n          if (seoData) {\n            this.seoData = seoData;\n          }\n        }\n      }\n    }\n  },\n  mounted() {\n    this.content = this.initialContent || \"\";\n\n    if (this.content) {\n      this.updateContent();\n    }\n  },\n  beforeDestroy() {\n    if (this.optimizationInterval) {\n      clearInterval(this.optimizationInterval);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.step-content {\n  background: white;\n  border-radius: 12px;\n  padding: 2rem;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n  color: #1c1c4c;\n  margin-bottom: 2rem;\n  text-align: center;\n}\n\n.editor-container {\n  display: grid;\n  grid-template-columns: 2fr 1fr;\n  gap: 2rem;\n}\n\n.main-content textarea {\n  width: 100%;\n  height: 400px;\n  padding: 1rem;\n  border: 1px solid #ddd;\n  border-radius: 6px;\n  resize: vertical;\n  font-family: \"Arial\", sans-serif;\n  line-height: 1.6;\n}\n\n.editor-actions {\n  display: flex;\n  justify-content: flex-